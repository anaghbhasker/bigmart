    <script>


function getCartlist() {
    
    $.ajax({
        url:`/cartlist`,
        method:"get",
        success:(response)=>{
            if (response.status==='success') {
              event.preventDefault()
              Swal.fire(
                     'BigMart',
                     'Please Login Your account',
                     'question'
              )
            }
            else{
    
              location.href="/cartlist"
            }
        }
    })
}


function getWishlist() {
    $.ajax({
      url: `/wishlist`,
          method: "get",
          success: (response)=>{
            if (response.status==='success') {
              event.preventDefault()
              Swal.fire(
                     'BigMart',
                     'Please Login Your account',
                     'question'
              )
            }
            else{
    
              location.href="/wishlist"
            }
          }
    })
}

    
    </script>